import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int fact = 1;
        for(int i=1;i<num;i++){
            fact *= i;
        }
        System.out.println("Enter the number:");
        System.out.println("factorial of " + num+ " is " + fact);
    }
}
