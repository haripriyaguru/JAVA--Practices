import java.util.*;
public class Main{
    public static void main(String args[]){
        int arr[] = {12,16,78,65,89,80};
        int small=arr[0];
        int big=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]>big){
                big=arr[i];
            }
            if(arr[i]<small){
                small=arr[i];
        }
    }
    System.out.println("Smallest Number:"+small);
    System.out.println("Biggest Number:"+big);
}
}
